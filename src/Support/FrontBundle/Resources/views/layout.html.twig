{% extends "::base.html.twig" %}

{% block title %}창업지원{% endblock %}
{% block stylesheets %}
    {% if isMobile == false %}
      <link rel="stylesheet" href="{{asset('bundles/support/css/common.css')}}"/>
        {% if getControllerName() == "mainpage" %}
             <link rel="stylesheet" href="{{asset('bundles/support/css/main.css')}}" />
        {% elseif getControllerName() == "product"%}
            {% if getActionName() != "detail" %}
                <link rel="stylesheet" href="{{asset('bundles/support/css/main.css')}}" />
            {% endif %}
        {% endif %}      
    {% else %}
      <link rel="stylesheet" href="{{asset('bundles/support/css/mobile.common.css')}}"/>        
    {% endif %}
<style type='text/css'>
    @font-face{
	font-family: "Nanum Gothic";
	src: url('{{asset("bundles/support/css/fonts/NanumGothic.eot")}}');
	src: url('{{asset("bundles/support/css/fonts/NanumGothic.eot?#iefix")}}') format('embedded-opentype'),
	url('{{asset("bundles/support/css/fonts/NanumGothic.woff")}}') format('woff'),
	url('{{asset("bundles/support/css/fonts/NanumGothic.ttf")}}') format('truetype');
	url('{{asset("bundles/support/css/fonts/NanumGothic.svg#NanumGothic")}}') format('svg')
	src: local(※), url('{{asset("bundles/support/css/fonts/NanumGothic.woff")}}') format('woff');
}
</style>
{% endblock %}

{% block body %}
    <div id="content">
        {% if (isApp == false) %}
        <div id="header">
            {% block header %}
               {% include "SupportFrontBundle::header.html.twig" %}     
            {% endblock %}
        </div>
        <div id="darkScreen"></div>
            {% block ad %}   
                {% if (isMobile == false) %}
                    <div id="adTop" class="ad">
                        {% if getControllerName() == "mainpage" %}
                            <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
                            <!-- AD_TOP -->
                            <ins class="adsbygoogle"
                                 style="display:inline-block;width:728px;height:90px"
                                 data-ad-client="ca-pub-2134771261384258"
                                 data-ad-slot="2433888321"></ins>
                            <script>
                            (adsbygoogle = window.adsbygoogle || []).push({});
                            </script> 
                        {% elseif getControllerName() == "product"%}
                            {% if getActionName() != "detail" %}
                                <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
                                <!-- AD_TOP -->
                                <ins class="adsbygoogle"
                                     style="display:inline-block;width:728px;height:90px"
                                     data-ad-client="ca-pub-2134771261384258"
                                     data-ad-slot="2433888321"></ins>
                                <script>
                                (adsbygoogle = window.adsbygoogle || []).push({});
                                </script> 
                            {% endif %}      
                        {% endif %}
                        </div>
                        <div id="adNav" class="ad">
                            <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
                            <!-- AD_NAV_SIDE -->
                            <ins class="adsbygoogle"
                                 style="display:inline-block;width:200px;height:200px"
                                 data-ad-client="ca-pub-2134771261384258"
                                 data-ad-slot="5387354725"></ins>
                            <script>
                            (adsbygoogle = window.adsbygoogle || []).push({});
                            </script>        
                        </div>
                {% endif %}  
            {% endblock %}        
        {% endif %}
        {% block content%}
             <div class="nav float-left">
                {% block nav %}       
                    {% include "SupportFrontBundle::navigation.html.twig" %}  
                    {% if (isMobile == true) and (isApp == false) %}
                    <div id="adModileTop">
                          {#  <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
                            <!-- ad_mobile_banner -->
                            <ins class="adsbygoogle"
                                 style="display:block"
                                 data-ad-client="ca-pub-7177895853185351"
                                 data-ad-slot="9456769620"
                                 data-ad-format="link"></ins>
                            <script>
                            (adsbygoogle = window.adsbygoogle || []).push({});
                            </script>#}
                    </div> 
                    {% endif %}                    
                {% endblock %}
            </div>
            <div class="contents float-left">
                {% block contents %}
                {% endblock %}
            </div>
        {% endblock %}    
    </div>
        {% block footer %}
            <div class="footer center">
           {% include "SupportFrontBundle::footer.html.twig" %}
           </div>
        {% endblock %}        
{% endblock %}

{% block javascripts %}
    <script src="{{asset('/bundles/support/js/jquery-2.1.4.min.js')}}" type="text/javascript"></script>
    <script src="{{asset('/bundles/support/js/jquery.cookie.js')}}" type="text/javascript"></script>
    <script type="text/javascript" src="{{asset("bundles/support/js/common.js")}}"></script>
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-74361671-1', 'auto');
        ga('send', 'pageview');

  </script>

    {% if isMobile == false %}
        <script type="text/javascript" src="{{asset("bundles/support/js/basic.code.js")}}"></script>
    {% else %}
        <script>
        var action = "{{getActionName()}}";
        var url;
        if(action == "all"){
            url = $("#navAll");
        }else if(action == "popular"){
                url =$("#navPop");
        }else if(action == "duedate"){
                url =$("#navDue");
        }else if(action == "customSearch"){
                url = $("#navCus");
                var id = "{{getCurrentSubNavId()}}";
                if(id == 0){
                    id = $("#subNavAll");
                }else if(id == 1){
                    id = $("#subNavAge");
                }else if(id == 2){
                    id = $("#subNavBus");
                }else if(id == 3){
                    id = $("#subNavReg");
                }
        }
        </script>
        <script type="text/javascript" src="{{asset("bundles/support/js/mobile.basic.code.js")}}"></script> 
    {% endif %}
    
{% endblock %}
